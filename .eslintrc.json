{
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": false
    }
  },

  "extends": [ "standard" ],

  "rules": {
    "no-func-assign": "off",
    "no-proto": "off",
    "no-undef": "warn",
    "no-unused-vars": "warn",
    "no-fallthrough": "off",
    "no-unused-expressions": "off",
    "no-sequences": "off",
    "array-bracket-spacing": "off"
  }
}
